{
    "DESIGN_NAME": "dffram1024x32_wrap",
    "VERILOG_FILES": "dir::rtl/*.sv",
   
    "CLOCK_PORT": "clk_i",
    "CLOCK_PERIOD": 25,
   
    "pdk::sky130A": {
      "MAX_FANOUT_CONSTRAINT": 6,
      "FP_CORE_UTIL": 40,
      "PL_TARGET_DENSITY_PCT": "expr::($FP_CORE_UTIL + 10.0)",
      "scl::sky130_fd_sc_hd": {
        "CLOCK_PERIOD": 25.0
      }
    },

    "FP_SIZING": "absolute",
    "DIE_AREA": [0, 0, 2000, 1400],
    "//": "PDN configurations",
    "FP_PDN_VOFFSET": 5,
    "FP_PDN_HOFFSET": 5,
    "FP_PDN_VWIDTH": 3.1,
    "FP_PDN_HWIDTH": 3.1,
    "FP_PDN_VSPACING": 15.5,
    "FP_PDN_HSPACING": 15.5,
    "FP_PDN_VPITCH": 50,
    "FP_PDN_HPITCH": 50,
    "QUIT_ON_PDN_VIOLATIONS": false,


    "//": "Hardening strategy variables (this is for 3-Top-Level Integration). Visit https://docs.google.com/document/d/1pf-wbpgjeNEM-1TcvX2OJTkHjqH_C9p-LURCASS0Zo8 for more info",
    "SYNTH_ELABORATE_ONLY": false,
    "RUN_POST_GPL_DESIGN_REPAIR": true,
    "RUN_POST_CTS_RESIZER_TIMING": true,
    "DESIGN_REPAIR_BUFFER_INPUT_PORTS": true,
    "FP_PDN_ENABLE_RAILS": true,
    "RUN_ANTENNA_REPAIR": true,
    "RUN_FILL_INSERTION": true,
    "RUN_TAP_ENDCAP_INSERTION": true,
    "RUN_CTS": true,
    "RUN_IRDROP_REPORT": false,
    
    "VDD_NETS": [
      "VPWR"
  ],
    "GND_NETS": [
        "VGND"
    ],

    "MACROS": {
      "RAM256": {
          "instances": {
          "dffram0": {
              "location": [100, 100],
              "orientation": "N"
          },
          "dffram1": {
              "location": [100, 750],
              "orientation": "FS"
          },
          "dffram2": {
              "location": [1000, 100],
              "orientation": "FN"
          },
          "dffram3": {
              "location": [1000, 750],
              "orientation": "S"
          }
        },
          "gds": [
              "dir::macros/dffram256x32/gds/RAM256.gds"
          ],
          "lef": [
              "dir::macros/dffram256x32/lef/RAM256.lef"
          ],
          "spef": {
              "*": [
                "dir::macros/dffram256x32/spef/max_/RAM256.max.spef"
              ]
          },
          "lib": {
              "*": "dir::macros/dffram256x32/lib/max_ss_100C_1v60/RAM256__max_ss_100C_1v60.lib"
          },
          "nl": [
            "dir::macros/dffram256x32/nl/RAM256.nl.v"
          ]
      }
     },
     "PDN_MACRO_CONNECTIONS": [
        "dffram0 VPWR VGND VPWR VGND",
        "dffram1 VPWR VGND VPWR VGND",
        "dffram2 VPWR VGND VPWR VGND",
        "dffram3 VPWR VGND VPWR VGND"
      ]
  }