    "DESIGN_REPAIR_MAX_SLEW_PCT": 30,
    "DESIGN_REPAIR_MAX_CAP_PCT": 30,
    "MAX_TRANSITION_CONSTRAINT": 1.5,

   
    "pdk::sky130A": {
      "MAX_FANOUT_CONSTRAINT": 6,
      "FP_CORE_UTIL": 40,
      "PL_TARGET_DENSITY_PCT": "expr::($FP_CORE_UTIL + 10.0)",
      "scl::sky130_fd_sc_hd": {
        "CLOCK_PERIOD": 30.0
      }
    },

    "FP_SIZING": "absolute",
